{
  "name": "@arkahna/git-file-fetch",
  "version": "0.1.0",
  "description": "Fetch specific files from remote git repos and track them locally.",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "git-file-fetch": "dist/index.js"
  },
  "scripts": {
    "start": "ts-node --esm src/index.ts",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.plugin.json && pnpm run build:plugin-assets",
    "build:plugin-assets": "node -e \"require('fs').mkdirSync('dist/plugin/executors/fetch', {recursive: true})\" && node -e \"require('fs').copyFileSync('plugin/executors.json', 'dist/plugin/executors.json')\" && node -e \"require('fs').copyFileSync('plugin/executors/fetch/schema.json', 'dist/plugin/executors/fetch/schema.json')\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test:smoke": "node dist/index.js \"https://github.com/octokit/core.js.git@main:LICENSE\" --dry-run --json",
    "prepack": "pnpm run build"
  },
  "keywords": [
    "git",
    "file",
    "fetch",
    "devtool",
    "remote",
    "cli"
  ],
  "author": "Arkahna",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arkahna/git-file-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/arkahna/git-file-fetch/issues"
  },
  "homepage": "https://github.com/arkahna/git-file-fetch#readme",
  "executors": "./plugin/executors.json",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^22.17.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "plugin"
  ],
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
